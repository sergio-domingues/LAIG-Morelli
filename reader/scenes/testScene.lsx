<SCENE>

<INITIALS>
    <frustum near="0.1" far="500"/>                      <!-- frustum planes-->
    <translation x="0" y="0" z="0" />                 <!-- initial translate -->
    <rotation axis="x" angle="0" />                  <!-- initial rotation 3 -->
    <rotation axis="y" angle="0" />                  <!-- initial rotation 2 -->
    <rotation axis="z" angle="0" />                  <!-- initial rotation 1 -->
    <scale sx="0.5" sy="0.5" sz="0.5" />                  <!-- initial scaling -->
    <reference length="10" />                          <!-- axis length; "0" means no axis-->
</INITIALS>

<ILLUMINATION>
    <ambient r="0" g="0" b="0" a="1.0" />            <!-- global ambient -->
    <background r="0.0" g="0.0" b="0" a="1.0" />         <!-- background color -->
</ILLUMINATION>

<LIGHTS>
    <LIGHT id="l01">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="0" y="6" z="7.5" w="1" />       <!-- light position -->
        <ambient r="0" g="0" b="0" a="1" />        <!-- ambient component -->
        <diffuse r="1" g="1" b="1" a="1" />        <!-- diffuse component -->
        <specular r="1" g="1" b="1" a="1" />       <!-- specular component -->
    </LIGHT>
	
	<LIGHT id="l02">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="5" y="4" z="7.5" w="1" />       <!-- light position -->
        <ambient r="0.5" g="0.5" b="0.5" a="1" />        <!-- ambient component -->
        <diffuse r="1" g="1" b="1" a="1" />        <!-- diffuse component -->
        <specular r="1" g="1" b="1" a="1" />       <!-- specular component -->
    </LIGHT>

    <LIGHT id="l03">                                    <!-- light identifier -->
        <enable value ="1" />                         <!-- enable/disable -->
        <position x="10" y="4" z="7.5" w="7.5" />       <!-- light position -->
        <ambient r="0.5" g="0.5" b="0.5" a="1" />        <!-- ambient component -->
        <diffuse r="1" g="1" b="1" a="1" />        <!-- diffuse component -->
        <specular r="1" g="1" b="1" a="1" />       <!-- specular component -->
    </LIGHT>

    <!-- NOTE: this block "LIGHT" must be repeated as necessary with different "id" -->
    
</LIGHTS>


<TEXTURES>
    <TEXTURE id="t01">									<!-- Table texture -->
        <file path="resources/table.png" />        <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
	

    <TEXTURE id="paisagem">								<!-- Paisagem texture -->
        <file path="resources/paisagem.png" />         <!-- path to file -->
        <amplif_factor s="15" t="7" />                <!-- x/s, y/t -->
    </TEXTURE>

     <TEXTURE id="floor">								<!-- Paisagem texture -->
        <file path="resources/wood-floor.png" />         <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="tapete">								<!-- Paisagem texture -->
        <file path="resources/tapete.png" />         <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="parede_japonesa">								<!-- Paisagem texture -->
        <file path="resources/parede_japonesa.png" />         <!-- path to file -->
        <amplif_factor s="15" t="7" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="porta_japonesa">								<!-- Paisagem texture -->
        <file path="resources/porta_japonesa.png" />         <!-- path to file -->
        <amplif_factor s="15" t="7" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="incenso">								<!-- Paisagem texture -->
        <file path="resources/incensoTexture.jpg" />         <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>



    <!-- NOTE: this block "TEXTURE" must be repeated as necessary with different "id" -->
    
</TEXTURES>


<MATERIALS>

    <MATERIAL id="m01">  								<!-- Wood -->                                
        <shininess value="120" />
        <specular r="0.1" g="0.1" b="0.1" a="1" />        <!-- specular reflection -->
        <diffuse r="0.4" g="0.2" b="0.2" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.4" g="0.2" b="0.2" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
     </MATERIAL>
	 
	 <MATERIAL id="m02">                                  	<!-- Metal --> 
        <shininess value="120" />
        <specular r="1" g="1" b="1" a="1" />        		<!-- specular reflection -->
        <diffuse r="0.73" g="0.75" b="0.8" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.73" g="0.75" b="0.8" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
     </MATERIAL>

     <MATERIAL id="yellow">                                  	<!-- Yellow --> 
        <shininess value="120" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />        		<!-- specular reflection -->
        <diffuse r="0.81" g="0.71" b="0.23" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.81" g="0.71" b="0.23" a="1" />         <!-- ambient reflection -->
        <emission r="0.4" g="0.4" b="0.4" a="1" />        		<!-- emission component -->
     </MATERIAL>

     <MATERIAL id="candeeiro">                                  	<!-- Yellow --> 
        <shininess value="120" />
        <specular r="1" g="1" b="1" a="1" />        		<!-- specular reflection -->
        <diffuse r="0.81" g="0.71" b="0.23" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.81" g="0.71" b="0.23" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        		<!-- emission component -->
     </MATERIAL>

     <MATERIAL id="preto">                                  	<!-- Yellow --> 
        <shininess value="120" />
        <specular r="0.1" g="0.1" b="0.1" a="1" />        		<!-- specular reflection -->
        <diffuse r="0.0" g="0.0" b="0.0" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.0" g="0.0" b="0.0" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        		<!-- emission component -->
     </MATERIAL>

      <MATERIAL id="branco">                                  	<!-- Yellow --> 
        <shininess value="120" />
        <specular r="1" g="1" b="1" a="1" />        		<!-- specular reflection -->
        <diffuse r="0.90" g="0.90" b="0.90" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.90" g="0.90" b="0.90" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        		<!-- emission component -->
     </MATERIAL>




    <!-- NOTE: the "MATERIAL" block may be repeated as required. Each defined material requires a distinct "id" -->

</MATERIALS>

